<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/gym9_ii"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/stefi_cohen_squats"
    android:alpha="1"
    tools:context=".gym9_ii">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:alpha="0.75"
            android:orientation="vertical">

            <Button
                android:id="@+id/b9iia"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:background="#FF5722"
                android:padding="5dp"
                android:text="Introduction"
                android:textAllCaps="false"
                android:textColor="#FFFFFF"
                android:textSize="20sp"
                android:textStyle="bold" />
            <Button
                android:id="@+id/b9iib"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:background="#FF9800"
                android:padding="5dp"
                android:text="Deadlifts"
                android:textAllCaps="false"
                android:textColor="#FFFFFF"
                android:textSize="20sp"
                android:textStyle="bold" />
            <Button
                android:id="@+id/b9iic"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:background="#FF5722"
                android:padding="5dp"
                android:text="Barbell Squats"
                android:textAllCaps="false"
                android:textColor="#FFFFFF"
                android:textSize="20sp"
                android:textStyle="bold" />
            <Button
                android:id="@+id/b9iid"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:background="#FF9800"
                android:padding="5dp"
                android:text="Lunges and Split Squats"
                android:textAllCaps="false"
                android:textColor="#FFFFFF"
                android:textSize="20sp"
                android:textStyle="bold" />
            <Button
                android:id="@+id/b9iie"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:background="#FF5722"
                android:padding="5dp"
                android:text="Bench Press"
                android:textAllCaps="false"
                android:textColor="#FFFFFF"
                android:textSize="20sp"
                android:textStyle="bold" />
            <Button
                android:id="@+id/b9iif"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:background="#FF9800"
                android:padding="5dp"
                android:text="Overhead Press (OHP)"
                android:textAllCaps="false"
                android:textColor="#FFFFFF"
                android:textSize="20sp"
                android:textStyle="bold" />
            <Button
                android:id="@+id/b9iig"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:background="#FF5722"
                android:padding="5dp"
                android:text="Rows"
                android:textAllCaps="false"
                android:textColor="#FFFFFF"
                android:textSize="20sp"
                android:textStyle="bold" />
            <Button
                android:id="@+id/b9iih"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:background="#FF9800"
                android:padding="5dp"
                android:text="Lat Pulldown"
                android:textAllCaps="false"
                android:textColor="#FFFFFF"
                android:textSize="20sp"
                android:textStyle="bold" />
            
        </LinearLayout>

    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>